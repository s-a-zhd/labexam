<!DOCTYPE html>
<html>
<head>
	<title>Edit page</title>
</head>
<body>
	<div>
		<h1>Edit Info</h1> 		
	</div>

	<a href="/home/alluser"> Back to List</a> |
	<a href="/logout"> logout</a><br>

	<form method="post">
		<table>
			<% for(var i=0; i < user.length; i++){ %>
			<tr>
				<td>Id</td>
				<td><input type="text" readonly name="id" value="<%=user[i].id%>"></td>
			</tr>
			<tr>
				<td>Username</td>
				<td><input type="text" name="username" value="<%=user[i].username%>"></td>
			</tr>
			<tr>
				<td>Password :</td>
				<td><input type="password" name="password" value="<%=user[i].password%>"></td>
			</tr>
			<tr>
				<td>type :</td>
				<td><input type="text" name="type" value="<%=user[i].type%>"></td>
			</tr>
			<tr>
				<td></td>
				<td><input type="submit" name="update" value="Update"></td>
			</tr>
			<% } %>
		</table>
	</form>
</body>
</html>